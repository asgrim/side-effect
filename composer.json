{
    "name": "asgrim/side-effect",
    "description": "Side Effect Framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Asgrim\\SideEffect\\": "src/"
        }
    },
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "require": {
        "php": "8.1.*",
        "ext-pdo": "*",
        "doctrine/coding-standard": "^9.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^2.4",
        "vimeo/psalm": "^4.26"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "conflict": {
        "laravel/laravel": "*",
        "laravel/framework": "*",
        "symfony/symfony": "*",
        "laminas/laminas-mvc": "*",
        "slim/slim": "*",
        "mezzio/mezzio": "*",
        "codeigniter/framework": "*",
        "yiisoft/yii2": "*",
        "cakephp/cakephp": "*",
        "phalcon/zephir": "*",
        "fuelphp/fuelphp": "*"
    }
}
